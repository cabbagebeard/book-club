<div class="fluid-container">
    
    <h1>Clubs I admin for</h1>
    <% current_user.memberships.each do |m| %>
        <% if m.admin == true then %>
            <% admin_club = current_user.clubs.find(m.club_id) %>
            <%= link_to admin_club.name, admin_club  %>
        <% end %>
    <% end %>
    
    <h1>Clubs I'm a member of</h1>

    
</div>
